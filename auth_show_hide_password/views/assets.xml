<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <template
        id="assets_frontend"
        inherit_id="web.assets_frontend"
        name="Auth Signup Show Hide Password"
    >
        <xpath expr="//script[last()]" position="after">
            <link
                rel="stylesheet"
                type="text/scss"
                href="/auth_show_hide_password/static/src/scss/signup_policy.scss"
            />
            <script
                type="text/javascript"
                src="/auth_show_hide_password/static/src/js/login.js"
            />
            <script
                type="text/javascript"
                src="/auth_show_hide_password/static/src/js/signup.js"
            />
            <script
                type="text/javascript"
                src="/auth_show_hide_password/static/src/js/reset_password.js"
            />
        </xpath>
    </template>
</odoo>
